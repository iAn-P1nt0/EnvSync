{
  "name": "@ian-p1nt0/envsync-core",
  "version": "0.1.0",
  "description": "EnvSync core detection engine - Environment parity validator",
  "author": "Ian Pinto <ian-p1nt0>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ipgthb/EnvSync.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
