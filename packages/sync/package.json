{
  "name": "@ian-p1nt0/envsync-sync",
  "version": "0.1.0",
  "description": "EnvSync environment synchronization engine",
  "author": "Ian Pinto <ian-p1nt0>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ipgthb/EnvSync.git",
    "directory": "packages/sync"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "dependencies": {
    "@ian-p1nt0/envsync-core": "^0.1.0",
    "dockerode": "^4.0.2"
  },
  "keywords": [
    "sync",
    "environment",
    "automation"
  ],
  "license": "MIT"
}
