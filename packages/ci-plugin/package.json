{
  "name": "@ian-p1nt0/envsync-ci-plugin",
  "version": "0.1.1",
  "description": "EnvSync CI/CD integration plugins",
  "author": "Ian Pinto <ian-p1nt0>",
  "repository": {
    "type": "git",
    "url": "https://github.com/iAn-P1nt0/EnvSync.git",
    "directory": "packages/ci-plugin"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "dependencies": {
    "@ian-p1nt0/envsync-core": "^0.1.0",
    "@ian-p1nt0/envsync-sync": "^0.1.0"
  },
  "keywords": [
    "ci-cd",
    "github-actions",
    "gitlab-ci"
  ],
  "license": "MIT"
}
